<link rel="stylesheet" href="/css/home2.css">



<div class="container-fluid">
    

    
        <div id="chat-user-list">
            <h1 id="chat-head">Chat Users</h1>

            <% for(u of all_users){%>
                <p>
                    <a id="userlist" href="/users/profile/<%= u.id %>">
                       ** <%= u.name %> **
                    </a>
                </p>
            <% } %>
        </div>
        <button id="chat-box" class="btn btn-warning"><a href="/users/chatbox">Go To ChatBox</a></button>

</div>

